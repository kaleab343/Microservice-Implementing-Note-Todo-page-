{
  "name": "micronote-api-gateway-hybrid",
  "version": "1.0.0",
  "description": "Hybrid API Gateway - REST to gRPC translator for MicroNote",
  "main": "server.js",
  "type": "module",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "proto:generate": "grpc_tools_node_protoc --js_out=import_style=commonjs,binary:./src/generated --grpc_out=grpc_js:./src/generated --plugin=protoc-gen-grpc=`which grpc_tools_node_protoc_plugin` -I ../../proto ../../proto/*.proto",
    "proto:ts": "grpc_tools_node_protoc --plugin=protoc-gen-ts=./node_modules/.bin/protoc-gen-ts --ts_out=grpc_js:./src/generated -I ../../proto ../../proto/*.proto"
  },
  "dependencies": {
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "@grpc/grpc-js": "^1.9.0",
    "@grpc/proto-loader": "^0.7.0",
    "http-proxy-middleware": "^2.0.6",
    "express-rate-limit": "^7.1.5",
    "helmet": "^7.1.0",
    "compression": "^1.7.4",
    "jsonwebtoken": "^9.0.2",
    "redis": "^4.6.10",
    "socket.io": "^4.7.0",
    "winston": "^3.8.2"
  },
  "devDependencies": {
    "nodemon": "^3.0.1",
    "grpc-tools": "^1.12.4"
  }
}